{
  "variables": {
    "vm_name": "Cloudspace-sharedfolders-14.04"
  },
  "builders": [{
    "type": "virtualbox-ovf",
    "vm_name": "{{user `vm_name`}}",
    "source_path": "virtualbox-ubuntu-14.04/cloudspace-ubuntu-14.04.ovf",
    "output_directory": "virtualbox-sharedfolders",
    "ssh_username": "cloudspace",
    "ssh_password": "cloudspace",
    "ssh_wait_timeout": "20m",
    "shutdown_command": "sudo shutdown -P now"
  }],
  "post-processors": ["vagrant"]
}